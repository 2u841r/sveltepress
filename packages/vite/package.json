{
  "name": "@svelte-press/vite",
  "version": "0.0.4",
  "description": "Sveltepress plugin for vite",
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/es/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./style.css": "./dist/style.css",
    "./CExpansion.svelte": "./dist/CExpansion.svelte"
  },
  "scripts": {
    "build:es": "tsc -p tsconfig.json -m es6 --outDir dist/es",
    "build:cjs": "tsc -p tsconfig.json -m commonjs --outDir dist/cjs",
    "build": "pnpm build:es && pnpm build:cjs && cp src/style.css dist && cp src/CExpansion.svelte dist"
  },
  "keywords": [
    "vite",
    "vite plugin",
    "sveltepress",
    "sveltepress vite",
    "sveltekit"
  ],
  "author": {
    "name": "Dongsheng Zhao",
    "email": "1197160272@qq.com"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/Blackman99/sveltepress",
    "directory": "packages/vite"
  },
  "license": "MIT",
  "devDependencies": {
    "@svelte-press/remark-live-code": "workspace:*",
    "@sveltejs/adapter-static": "^1.0.0",
    "@types/node": "^18.11.18",
    "picocolors": "^1.0.0",
    "svelte": "^3.55.0",
    "typescript": "^4.9.4",
    "unocss": "^0.48.0",
    "vite": "^4.0.3",
    "vite-plugin-inspect": "^0.7.11"
  },
  "dependencies": {
    "@iconify/json": "^2.1.157",
    "@sveltejs/kit": "^1.0.0"
  }
}
