{
  "name": "@svelte-press/vite",
  "version": "0.0.2",
  "description": "Sveltepress plugin for vite",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "import": "./dist/es/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "build:es": "tsc -p tsconfig.json -m es6 --outDir dist/es",
    "build:cjs": "tsc -p tsconfig.json -m commonjs --outDir dist/cjs",
    "build": "pnpm build:es && pnpm build:cjs"
  },
  "keywords": [
    "vite",
    "vite plugin",
    "sveltepress",
    "sveltepress vite",
    "sveltekit"
  ],
  "author": {
    "name": "Dongsheng Zhao",
    "email": "1197160272@qq.com"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/Blackman99/sveltepress",
    "directory": "packages/vite"
  },
  "license": "MIT",
  "devDependencies": {
    "@svelte-press/remark-live-code": "workspace:*",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@types/node": "^18.11.18",
    "svelte": "^3.55.0",
    "typescript": "^4.9.4",
    "unocss": "^0.48.0",
    "vite": "^4.0.3"
  }
}
