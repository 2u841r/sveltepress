{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"moduleResolution": "node",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"lib": ["ESNext"],
		"module": "ESNext"
	},
	"include": [
		"node_modules/@svelte-press/vite/types/sveltepress.d.ts"
	]
}
